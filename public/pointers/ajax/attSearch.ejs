<head>
    <style>
        .results{
            display: flex;
            flex-direction: column;
            width: 100%;
        }

        .results > a{
            text-decoration: none;
            height: 52px;
            margin: 10px 0px;
            color: black;
            background-color: rgb(255, 255, 255);
            transition: 0.1s ease-in;
        }

        .results > a:hover{
            background-color: rgb(235, 235, 235);
            transition: 0.1s ease-in;
        }

        .results > a > .content{
            display: flex;
            align-items: center;
        }

        .results > a > .content > img{
            width: 50px;
            height: 52px;
            margin-right: 15px;
        }

        .search-window.show{
            display: flex;
        }
    </style>
</head>
<body>
    <div class="results" id="show-results">
        <% for(profiles of db) { %>
            <a href="/Profile?id=<%= profiles.profile_id %>">
                <div class="content">
                    <img src="<%= profiles.photo %>" alt="">
                    <span><%= profiles.first_name %></span>
                </div>
            </a>
        <% } %>
    </div>
</body>